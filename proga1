def read(n):
    a = []
    for i in range(0, n):
        try:
            a.append(float(input()))

        except:
            print("Invalid type.")
            exit(0)
    return (a);
def min_product(a):
    min = a[0]*a[1]
    for i in range(0, len(a)-1):
        if a[i]*a[i+1] < min:
            min = a[i]*a[i+1]
    return min
try:
    n=int(input())
except:
    print("Invalid type.")
    exit(0)
a=read(n);
print(min_product(a))
